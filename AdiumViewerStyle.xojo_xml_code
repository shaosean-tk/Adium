 <Property>
  <ItemName>Template</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Template As String</SourceLine>
   <SourceLine></SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <ItemDeclaration>Template As String</ItemDeclaration>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <SetAccessor>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Set</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Set</SourceLine>
  </SetAccessor>
  <GetAccessor>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Get</SourceLine>
   <SourceLine>DIM tReturn As String = me.kTemplate</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (me.ResourcesFolder.Child("Template.html").Exists) then</SourceLine>
   <SourceLine>tReturn = me.ResourcesFolder.Child("Template.html").OpenAsText</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// set the base href path</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", me.ResourcesFolder.URLPath)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// set the css</SourceLine>
   <SourceLine>if (tReturn.CountFields("%@") = 5) then</SourceLine>
   <SourceLine>if (me.ResourcesFolder.Child("main.css").Exists) then</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", "@import url(""main.css"");")</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", "")  // this is to workaround themes that do not have a main.css file</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>' if (me.VariantPath &lt;&gt; Nil) AND (me.VariantPath.Name &lt;&gt; "main.css") then</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", me.VariantPath.URLPath)</SourceLine>
   <SourceLine>' else</SourceLine>
   <SourceLine>' tReturn = tReturn.Replace("%@", "")</SourceLine>
   <SourceLine>' end if</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>if (me.VariantPath &lt;&gt; Nil) AND (me.VariantPath.Name &lt;&gt; "main.css") then</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", "variants/" + me.VariantPath.Name)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", "main.css")</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// header</SourceLine>
   <SourceLine>' if (ShowHeader) then</SourceLine>
   <SourceLine>' tReturn = tReturn.Replace("%@", me.Header)</SourceLine>
   <SourceLine>' else</SourceLine>
   <SourceLine>tReturn = tReturn.Replace("%@", "")</SourceLine>
   <SourceLine>' end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// footer</SourceLine>
   <SourceLine>' if (ShowFooter) then</SourceLine>
   <SourceLine>' tReturn = tReturn.Replace("%@", me.Footer)</SourceLine>
