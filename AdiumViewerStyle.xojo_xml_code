  <ItemName>Constructor</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub Constructor(path As FolderItem)</SourceLine>
   <SourceLine>me.mPath = path</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (InStr(path.Name, ".css") &gt; 0) then  // loading with a variant path</SourceLine>
   <SourceLine>me.mStylePath = MessageStylePathForVariant(path)</SourceLine>
   <SourceLine>me.mVariantPath = path</SourceLine>
   <SourceLine>else  // loading with the .AdiumMessageStyle path</SourceLine>
   <SourceLine>me.mStylePath = path</SourceLine>
   <SourceLine>me.mVariantPath = Nil</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>DIM infoDocument As NEW XmlDocument(me.ContentsFolder.Child("Info.plist"))</SourceLine>
   <SourceLine>DIM documentNode As XmlNode = infoDocument.FirstChild.FirstChild</SourceLine>
   <SourceLine>DIM childNode As XmlNode = documentNode.FirstChild</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while childNode &lt;&gt; Nil</SourceLine>
   <SourceLine>DIM key As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (childNode.Name = "key") then</SourceLine>
   <SourceLine>key = "m" + Replace(childNode.FirstChild.Value, " ", "")</SourceLine>
   <SourceLine>childNode = childNode.NextSibling</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (childNode.Name = "true") OR (childNode.Name = "false") then</SourceLine>
   <SourceLine>me.SetValueForKey (childNode.Name = "true"), key</SourceLine>
   <SourceLine>elseif (childNode.Name = "integer") then</SourceLine>
   <SourceLine>me.SetValueForKey Val(childNode.FirstChild.Value), key</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>me.SetValueForKey childNode.FirstChild.Value, key</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>childNode = childNode.NextSibling</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>me.LoadVariants</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
