 <Method>
  <ItemName>Escape</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function Escape(value As String) As String</SourceLine>
   <SourceLine>value = ReplaceAll(value, "\", "\\")   // slashes</SourceLine>
   <SourceLine>value = ReplaceAll(value, """", "\""")  // quotes</SourceLine>
   <SourceLine>value = ReplaceAll(value, "'", "\'")     // apostrophes</SourceLine>
   <SourceLine>value = ReplaceAll(value, Chr(9), "") // TAB</SourceLine>
   <SourceLine>value = ReplaceLineEndings(value, "")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return value</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>value As String</ItemParams>
  <ItemResult>String</ItemResult>
 </Method>
 <Method>
  <ItemName>LoadVariants</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub LoadVariants()</SourceLine>
   <SourceLine>if (me.VariantPath = Nil) then</SourceLine>
   <SourceLine>if (me.MessageViewVersion &lt; 3) then  // view versions 0, 1 or 2</SourceLine>
   <SourceLine>me.mVariantPath = me.ResourcesFolder.Child("main.css")</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>if (me.DefaultVariant = "") then</SourceLine>
   <SourceLine>me.mVariantPath = me.ResourcesFolder.Child("main.css")</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>me.mVariantPath = me.VariantsFolder.Child(me.DefaultVariant + ".css")</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
