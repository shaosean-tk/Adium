  <ItemName>SetValueForKey</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub SetValueForKey(value As Variant, key As String)</SourceLine>
   <SourceLine>DIM properties() As Introspection.PropertyInfo = Introspection.GetType(me).GetProperties</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (UBound(properties) &gt; -1) then</SourceLine>
   <SourceLine>for i as Integer = 0 to UBound(properties)</SourceLine>
   <SourceLine>if (properties(i).Name = key) then</SourceLine>
   <SourceLine>properties(i).Value(me) = value</SourceLine>
   <SourceLine>exit for i</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>next</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
